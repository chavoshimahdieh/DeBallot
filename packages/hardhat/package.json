{
  "name": "@rarimo/voting-contracts",
  "version": "0.1.1",
  "license": "MIT",
  "author": "Zero Block Global Foundation",
  "description": "This repository contains contracts for managing the anonymous registration and voting processes.",
  "bugs": {
    "url": "https://github.com/rarimo/voting-contracts/issues"
  },
  "homepage": "https://github.com/rarimo/voting-contracts#readme",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rarimo/voting-contracts.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart-contracts"
  ],
  "files": [
    "**/*.sol",
    "!mock/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "account": "hardhat run scripts/listAccount.ts",
    "chain": "hardhat node --network hardhat --no-deploy",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy",
    "fork": "MAINNET_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
    "generate": "hardhat run scripts/generateAccount.ts",
    "flatten": "hardhat flatten",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "format": "prettier --write ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify",
    "hardhat-verify": "hardhat verify",
    "clean": "rm -rf ./artifacts && rm -rf ./cache && rm -rf ./hardhat-deploy && rm -rf ./deployments && rm -rf ./typechain-types && forge clean",
    "anvil": "make anvil",
    "build": "forge build",
    "ftest": "forge test",
    "fdeploy": "make deploy-anvil",
    "dashboard": "truffle dashboard",
    "tdeploy": "hardhat deploy --network truffle-dashboard --export-all ./temp/hardhat_contracts.json \"$@\" && hardhat run utils/generateTsAbis.ts",
    "prepare": "husky",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.ts",
    "private-network": "npx hardhat node",
    "deploy-localhost": "npx hardhat migrate --network localhost",
    "deploy-sepolia": "npx hardhat migrate --network sepolia --verify --verify-attempts 10",
    "deploy-qDevnet": "npx hardhat migrate --network qDevnet --verify --verify-attempts 10",
    "deploy-qTestnet": "npx hardhat migrate --network qTestnet --verify --verify-attempts 10",
    "deploy-qMainnet": "npx hardhat migrate --network qMainnet --verify --verify-attempts 10",
    "generate-types": "TYPECHAIN_FORCE=true npx hardhat typechain",
    "generate-docs": "npx hardhat markup",
    "lint-fix": "npm run lint-sol-fix && npm run lint-ts-fix && npm run lint-json-fix",
    "lint-json-fix": "prettier --write \"./**/*.json\"",
    "lint-ts-fix": "prettier --write \"./**/*.ts\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\"",
    "publish-to-npm": "npm run lint-fix && bash ./scripts/publish.sh --public"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.6.1",
    "@iden3/contracts": "1.4.7",
    "@openzeppelin/contracts": "4.9.5",
    "@openzeppelin/contracts-upgradeable": "4.9.5",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@pancakeswap/pancake-swap-lib": "^0.0.4",
    "@rarimo/evm-bridge-contracts": "1.0.3",
    "@solarity/solidity-lib": "2.7.3",
    "@wallet-standard/wallet": "^1.0.1",
    "big.js": "^6.2.1",
    "circomlib": "^2.0.5",
    "eip-712": "^1.0.0",
    "envfile": "^6.18.0",
    "eth-sig-util": "^3.0.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers-utils": "^2.1.11",
    "hardhat-console": "^1.0.0",
    "hardhat-laika": "^0.1.1",
    "hardhat-slither": "^1.0.4",
    "node-forge": "^1.3.1",
    "qrcode": "^1.5.1",
    "solidity-bytes-utils": "0.8.2",
    "solidity-code-metrics": "^0.0.25"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@iden3/js-crypto": "^1.0.3",
    "@iden3/js-iden3-auth": "^1.1.1",
    "@iden3/js-iden3-core": "^1.2.1",
    "@iden3/js-jsonld-merklization": "^1.1.2",
    "@iden3/js-merkletree": "^1.1.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.4",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-foundry": "^1.1.2",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "@nomicfoundation/ignition-core": "^0.15.5",
    "@solarity/hardhat-habits": "^1.0.0",
    "@solarity/hardhat-markup": "^1.0.5",
    "@solarity/hardhat-migrate": "^2.1.10",
    "@truffle/dashboard-hardhat-plugin": "^0.2.15",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/eslint": "^8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.10",
    "@types/prettier": "^2",
    "@types/qrcode": "^1",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "4.4.1",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.4.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eth-gas-reporter": "^0.2.27",
    "ethers": "^6.10.0",
    "hardhat": "^2.22.9",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.45",
    "hardhat-deploy-ethers": "^0.4.1",
    "hardhat-gas-reporter": "^2.1.1",
    "hardhat-preprocessor": "^0.1.5",
    "husky": "^9.0.7",
    "mocha": "10.2.0",
    "mock-local-storage": "^1.1.24",
    "nc": "^1.0.3",
    "prettier": "^3.3.2",
    "prettier-plugin-solidity": "^1.3.1",
    "snarkjs": "^0.7.3",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "0.8.11",
    "truffle": "^5.11.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.5.4"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
